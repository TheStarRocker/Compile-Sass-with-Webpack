{
  "name": "compile-sass",
  "version": "1.0.0",
  "description": "Webpack environment ready to compile sass.",
  "main": "index.js",
  "author": "Arian Vega",
  "license": "GPL-3.0-only",
  "scripts": {
    "tscc": "rimraf src/dist && tsc --build tsconfig.json",
    "dev": "rimraf dist && webpack --config src/dist/webpack/dev.js && rimraf dist/js",
    "prod": "rimraf dist/*.min.css && webpack --config src/dist/webpack/prod.js && rimraf dist/js",
    "clean": "rimraf dist && rimraf src/dist"
  },
  "devDependencies": {
    "@types/autoprefixer": "^9.7.2",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node": "^14.0.1",
    "autoprefixer": "^9.7.6",
    "css-loader": "^3.5.3",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
